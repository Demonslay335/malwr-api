{
    "name": "demonslay335/malwr-api",
    "description": "VirusTotal Public API v2.0 PHP Wrapper",
    "type": "library",
    "keywords": ["malwr", "api", "php", "wrapper", "security"],
    "homepage": "https://github.com/demonslay335/malwr-api",
    "authors": [
        {
            "name": "Michael Gillespie",
            "email": "demonslay335@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "guzzle/guzzle": ">=3.6"
    },
    "require-dev": {
       "phpunit/phpunit": ">=4.0"
    },
    "autoload": {
        "psr-0": {
            "Malwr": "src/",
            "Tests\\Malwr": "tests/"
        }
    },
    "config": {
        "vendor-dir" : "vendors",
        "vendor-bin" : "vendors/bin"
    }
}